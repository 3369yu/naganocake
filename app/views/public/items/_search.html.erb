<table class="search border border-2 w-100">
  <tr class="border-bottom text-center"><th>ジャンル検索</th></tr>
  <%= form_with url: items_path, method: :get do |f| %>
    <% genres.each do |genre| %>
    <tr><td><%= link_to genre.name, items_path(genre_id: genre.id) %></td></tr>
    <% end %>
  <% end %>
</table>